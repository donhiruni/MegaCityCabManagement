<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>loginServlet</servlet-name>
        <servlet-class>cab.loginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addBooking</servlet-name>
        <servlet-class>cab.addBooking</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registerCustomer</servlet-name>
        <servlet-class>cab.registerCustomer</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>manageCars</servlet-name>
        <servlet-class>cab.manageCars</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>manageDrivers</servlet-name>
        <servlet-class>cab.manageDrivers</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>profileServlet</servlet-name>
        <servlet-class>cab.profileServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loginServlet</servlet-name>
        <url-pattern>/loginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addBooking</servlet-name>
        <url-pattern>/addBooking</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registerCustomer</servlet-name>
        <url-pattern>/registerCustomer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>manageCars</servlet-name>
        <url-pattern>/manageCars</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>manageDrivers</servlet-name>
        <url-pattern>/manageDrivers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>profileServlet</servlet-name>
        <url-pattern>/profileServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
